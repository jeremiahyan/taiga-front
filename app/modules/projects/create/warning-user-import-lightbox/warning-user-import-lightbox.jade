tg-lightbox-close(on-close="onClose()")

.warning-users-import-lightbox
    h1.warning-users-import-title(translate="PROJECT.IMPORT.WARNING.TITLE")
    p(translate="PROJECT.IMPORT.WARNING.DESCRIPTION")
    .actions
        button.btn-small(
            variant="secondary",
            translate="PROJECT.IMPORT.WARNING.CHECK",
            ng-click="onClose()"
        )
        button.btn-small(
            variant="primary",
            type="submit",
            translate="PROJECT.IMPORT.IMPORT",
            ng-click="onConfirm()"
        )
